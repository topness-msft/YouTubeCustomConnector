  /youtube/v3/commentThreads:
    get:
      summary: Comments For Channel
      description: Get comments for a specified video or comma separated list of videos,
        or a channel.  Ordered with most recent comments first.
      operationId: CommentList
      parameters:
      - {name: part, default: 'id,snippet', in: query, type: string, required: true,
        x-ms-visibility: internal}
      - {name: allThreadsRelatedToChannelId, default: ZLgvFTOBuSk, in: query, type: string,
        required: false, x-ms-summary: Channel Id, description: YouTube channel Id,
        x-ms-visibility: important}
      - {name: videoId, default: ZLgvFTOBuSk, in: query, type: string, required: false,
        x-ms-summary: Video Id, description: 'YouTube video Id (ex: dQw4w9WgXcQ)',
        x-ms-visibility: important}
      - {name: maxResults, in: query, type: integer, required: true, x-ms-summary: Max
          Results, description: 1 to 100 inclusive, format: int32}
      - {name: pageToken, in: query, type: string, required: false}
      responses:
        '200':
          description: '200'
          schema:
            type: object
            properties:
              kind: {type: string, description: kind, title: '', x-ms-visibility: internal}
              etag: {type: string, description: etag, title: '', x-ms-visibility: internal}
              nextPageToken: {type: string, description: nextPageToken}
              prevPageToken: {type: string, description: prevPageToken}
              pageInfo:
                type: object
                properties:
                  totalResults: {type: integer, format: int32, description: totalResults,
                    title: Total Results}
                  resultsPerPage: {type: integer, format: int32, description: resultsPerPage,
                    title: Results Per Page}
                description: pageInfo
              items:
                type: array
                items:
                  type: object
                  properties:
                    kind: {type: string, description: kind, title: '', x-ms-visibility: internal}
                    etag: {type: string, description: etag, title: '', x-ms-visibility: internal}
                    id: {type: string, description: id}
                    snippet:
                      type: object
                      properties:
                        videoId: {type: string, description: videoId, title: Video
                            Id}
                        topLevelComment:
                          type: object
                          properties:
                            kind: {type: string, description: kind, title: '', x-ms-visibility: internal}
                            etag: {type: string, description: etag, title: '', x-ms-visibility: internal}
                            id: {type: string, description: id, title: Comment Id}
                            snippet:
                              type: object
                              properties:
                                videoId: {type: string, description: videoId, title: Video
                                    Id}
                                textDisplay: {type: string, description: textDisplay,
                                  title: Displayed Text}
                                textOriginal: {type: string, description: textOriginal,
                                  title: Original Text}
                                authorDisplayName: {type: string, description: authorDisplayName,
                                  title: Author Display Name}
                                authorProfileImageUrl: {type: string, description: authorProfileImageUrl,
                                  title: Author Profile Image URL}
                                authorChannelUrl: {type: string, description: authorChannelUrl,
                                  title: Author Channel URL}
                                authorChannelId:
                                  type: object
                                  properties:
                                    value: {type: string, description: value, title: Author
                                        Channel Id}
                                  description: authorChannelId
                                canRate: {type: boolean, description: canRate, title: '',
                                  x-ms-visibility: internal}
                                viewerRating: {type: string, description: viewerRating,
                                  title: Viewer Rating}
                                likeCount: {type: integer, format: int32, description: likeCount}
                                publishedAt: {type: string, description: publishedAt,
                                  title: Published At, format: date-time}
                                updatedAt: {type: string, description: updatedAt,
                                  title: Updated At, format: date-time}
                              description: snippet
                          description: topLevelComment
                        canReply: {type: boolean, description: canReply}
                        totalReplyCount: {type: integer, format: int32, description: totalReplyCount,
                          title: Total Reply Count}
                        isPublic: {type: boolean, description: isPublic, title: Is
                            Public}
                      description: snippet
                description: items
  /youtube/v3/playlistItems:
    get:
      summary: Get Playlist
      description: Get the contents of a playlist.
      operationId: GetPlaylist
      parameters:
      - {name: part, default: 'id,snippet,status', in: query, type: string, required: true,
        x-ms-visibility: internal}
      - {name: maxResults, default: 50, in: query, type: integer, required: false}
      - {name: pageToken, in: query, type: string, required: false}
      - name: playlistId
        in: query
        type: string
        required: true
        x-ms-dynamic-values:
          operationId: ListAllPlaylists
          value-path: id
          value-collection: items
          value-title: snippet/title
          parameters: {part: 'id,status,snippet', mine: true, maxResults: 50}
        x-ms-summary: Playlist Id
        description: 'The playlist Id (ex: PLYdUXTAVSHfSX3qO1LyJW4wGFmuiNGG0p)'
        x-ms-visibility: important
      responses:
        '200':
          description: '200'
